<div class="container">
  <div class="row">
    <div class><h1>Edit Page</h1></div>
  </div>

    <%= form_for @user do |f| %>
    <div>
      <%= f.text_field :profile_pic, placeholder: "Profile Photo" %>
    </div>
    <div>
      <%= f.text_field :first_name, placeholder: "First Name" %>
    </div>
    <div>
      <%= f.text_field :last_name, placeholder: "Last Name" %>
    </div>
    <div>
      <%= f.text_field :age, placeholder: "Age" %>
    </div>
    <div>
      <%= f.email_field :email, placeholder: "Email" %>
    </div>
    <div>
      <%= f.password_field :password, placeholder: "Password" %>
    </div>
    <div>
      <%= f.password_field :password_confirmation, placeholder: "Password" %>
    </div>
    <div>
      <%= f.text_field :current_city, placeholder: "Current City" %>
    </div>
    <div>
      <%= f.text_field :description, placeholder: "Description" %>
    </div>
    <%= f.submit "Edit" %>
  <% end %>
</div>